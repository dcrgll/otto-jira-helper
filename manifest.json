{
  "manifest_version": 3,
  "name": "Otto - Jira Helper",
  "description": "A helper for accessing Jira tickets",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "otto.png"
  },
  "omnibox": { "keyword": "otto" },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["storage"]
}
